module i_rxClkgen(
    input reset,
    input locked,
  //  input rxclk_2x,
    input rxclk,
    input rxclk_in,
    input rxclk_180
);

assert property(@(posedge rxclk_in)  (reset == 1) |-> (locked == 0));assert property(@(posedge rxclk_in)  (reset == 0) |-> (locked == 1));assert property(@(posedge rxclk_in)  (rxclk == rxclk_in) |-> (rxclk_180 == ~rxclk));assert property(@(posedge rxclk_in)  (rxclk_180 == ~rxclk) |-> (rxclk == rxclk_in));
endmodule