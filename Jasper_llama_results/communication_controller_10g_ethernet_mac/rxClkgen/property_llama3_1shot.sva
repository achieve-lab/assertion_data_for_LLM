module i_rxClkgen(
    input reset,
    input locked,
  //  input rxclk_2x,
    input rxclk,
    input rxclk_in,
    input rxclk_180
);

assert property(@(posedge rxclk_in) (reset) |-> (locked));
assert property(@(posedge rxclk_in)              (rxclk_in) |-> (rxclk));
assert property(@(posedge rxclk_in) (rxclk_in) |-> (rxclk_180));

endmodule