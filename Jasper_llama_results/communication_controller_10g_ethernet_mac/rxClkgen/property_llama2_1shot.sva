module i_rxClkgen(
    input reset,
    input locked,
  //  input rxclk_2x,
    input rxclk,
    input rxclk_in,
    input rxclk_180
);

assert property(@(posedge rxclk_in) (~reset) |-> (rxclk_180 == ~rxclk));
assert property(@(posedge rxclk_in) (~reset) |-> (locked == 1));
assert property(@(posedge rxclk_in) (reset) |-> (locked == 0));
assert property(@(posedge rxclk_in) (~reset) |-> (rxclk == rxclk_in));

endmodule