module i_eth_rxaddrcheck(
 input RxCheckEn,
  input [47:0] MAC,
  input        Reset, 
  input        ByteCntEq7,
  input        PassAll,
  input [1:0]  StateData,
  input        ByteCntEq6,
 input ByteCntEq2,
  input        ByteCntEq0,
  input [7:0]  RxData, 
 input UnicastOK,
  input [31:0] HASH1, 
 input RxAddressInvalid,
  input        r_Bro, 
  input        MRxClk, 
 input MulticastOK,
 input HashBit,
 input AddressMiss,
  input [31:0] HASH0, 
  input        ByteCntEq5,
  input        ByteCntEq3,
  input [5:0]  CrcHash, 
 input ByteCntEq4, 
  input        r_Pro, 
  input        Broadcast, 
  input       RxAbort,
 input [7:0]  ByteHash,
 input [31:0] IntHash,
 input BroadcastOK,
  input        RxEndFrm,
  input        CrcHashGood, 
  input        ControlFrmAddressOK,
  input        Multicast 
);

assert property(@(posedge MRxClk) (MulticastOK) |-> (Multicast));
assert property(@(posedge MRxClk) (UnicastOK) |-> (~MulticastOK));
assert property(@(posedge MRxClk) (BroadcastOK) |-> (~UnicastOK));
assert property(@(posedge MRxClk) (UnicastOK) |-> (~BroadcastOK));
assert property(@(posedge MRxClk) (RxCheckEn) |-> (~AddressMiss));
assert property(@(posedge MRxClk) (RxCheckEn) |-> (~RxAddressInvalid));
assert property(@(posedge MRxClk) (RxCheckEn) |-> (~RxAbort));
assert property(@(posedge MRxClk) (BroadcastOK) |-> (~MulticastOK));

endmodule