property a0;
@(posedge MRxClk) (ByteCntEq0 == 1) |=> (AddressMiss == 0);
endproperty
assert_a0: assert property(a0);

property a3;
@(posedge MRxClk) (BroadcastOK == 1 & ByteCntEq7 == 1 & RxCheckEn == 1) |=> (AddressMiss == 0);
endproperty
