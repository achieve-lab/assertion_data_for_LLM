property a25;
@(posedge MRxClk) (RxAddressInvalid == 0) |=> (RxAbort == 0);
endproperty
assert_a25: assert property(a25);

property a24;
@(posedge MRxClk) (RxAddressInvalid == 1 & ByteCntEq7 == 1 & RxCheckEn == 1) |=> (RxAbort == 1);
endproperty
assert_a24: assert property(a24);

property a23;
@(posedge MRxClk) (RxCheckEn == 0) |=> (RxAbort == 0);
endproperty
