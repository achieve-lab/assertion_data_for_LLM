module i_eth_l3_checksum(
input   [15:0]   CheckSum,
input    Reset,
input    MRxClk,
input [15:0] ByteCnt,
input            Divided_2_clk ,
input        CSready,
input   [1:0]    StartCalc,
input            Divided_4_clk ,
input   [31:0]   Sum,
input [7:0] prev_latched_Rx,
input [7:0] prev_latched_Rx1,
input [7:0] RxData
);

assert property(@(posedge MRxClk) (StartCalc[1] == 1 & StartCalc[0] == 1) |-> (CSready == 1));
assert property(@(posedge MRxClk) (StartCalc[1] == 1 & StartCalc[0] == 1) |-> (CheckSum == ~(Sum[15:0]+Sum[31:16])));

endmodule