parameter RESET_VALUE =0;parameter WIDTH =8 ;
module i_can_register_asyn_syn(
input             rst_sync,
input             rst,
input [WIDTH-1:0] data_out,
input             clk,
input [WIDTH-1:0] data_in,
input             we
);

assert property(@(posedge clk) (~we) |-> (data_out == $past(data_out)));
assert property(@(posedge clk) (we) |-> (data_out == data_in));

endmodule