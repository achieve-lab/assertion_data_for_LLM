module i_clean_rst(
  input rstt,
  input      rsti,
  input      clk,
  input  rsto
);

assert property(@(posedge clk) (rsti) |-> rstt);
assert property(@(posedge clk) (rstt) |-> rsti);
assert property(@(posedge clk) (rstt) |-> rsto);
assert property(@(posedge clk) (rstt ##1 1) |-> rsto);
assert property(@(posedge clk) (rsti) |-> rsto);
assert property(@(posedge clk) (rsti ##1 1) |-> rsto);
endmodule
