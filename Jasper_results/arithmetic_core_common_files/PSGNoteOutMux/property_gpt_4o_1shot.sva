parameter WID = 12;
module i_PSGNoteOutMux(
input [WID-1:0] o1,o2,o3,o4,o5,
input [WID-1:0] o,
input DEFAULT_CLOCK,
input DEFAULT_RESET,
input [4:0] s,
input [WID-1:0] a,b,c,d,e
);

assert property(@(posedge clk) (s[4] == 1) |-> (o1 == e);
assert property(@(posedge clk) (s[4] == 0) |-> (o1 == {WID{1'b1}});
assert property(@(posedge clk) (s[3] == 1) |-> (o2 == d);
assert property(@(posedge clk) (s[3] == 0) |-> (o2 == {WID{1'b1}});
assert property(@(posedge clk) (s[2] == 1) |-> (o3 == c);
assert property(@(posedge clk) (s[2] == 0) |-> (o3 == {WID{1'b1}});
assert property(@(posedge clk) (s[1] == 1) |-> (o4 == b);
assert property(@(posedge clk) (s[1] == 0) |-> (o4 == {WID{1'b1}});
assert property(@(posedge clk) (s[0] == 1) |-> (o5 == a);
assert property(@(posedge clk) (s[0] == 0) |-> (o5 == {WID{1'b1}});
assert property(@(posedge clk) (o == (o1 & o2 & o3 & o4 & o5));
endmodule