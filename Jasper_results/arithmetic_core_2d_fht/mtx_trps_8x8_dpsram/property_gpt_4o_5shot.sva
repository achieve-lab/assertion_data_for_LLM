parameter N = 8;
module i_mtx_trps_8x8_dpsram(
input			sclk,
input [15:0] rd_DATA,
input		indicator,				// 64 words written - Indication(pos. or neg. edge)
input			mem_valid,
input		cnt128d_rd_valid_start,	// Counter start increment
input [6:0]	cnt128d_wr,				// Write Mode Counter
input	[N-1:0]	inp_data,
input	[N-1:0]	mem_data,
input		wr_WEN,
input			rstn,
input			indicator_1d,			// Indication 1 clock delay
input		rd_CSN,
input		indicator_pos_edge,		// positive edge
input [ 6:0] wr_ADDR,
input [15:0] wr_DATA,
input		cnt128d_rd_valid_stop,	// Counter stop increment
input		wr_CSN,
input		indicator_neg_edge,		// negative edge
input			inp_valid,
input	[6:0]	cnt128d_rd,				// Read Counter
input [ 6:0] rd_ADDR,
input			cnt128d_rd_valid		// valid time for cnt128d_rd counter
);

assert property(@(posedge sclk) (!rstn) |-> (cnt128d_wr == 0));
assert property(@(posedge sclk) (inp_valid) |-> (cnt128d_wr == $past(cnt128d_wr) + 1));
assert property(@(posedge sclk) (!rstn) |-> (indicator_1d == 0));
assert property(@(posedge sclk) (indicator_pos_edge) |-> (cnt128d_rd_valid_start));
assert property(@(posedge sclk) (indicator_neg_edge) |-> (cnt128d_rd_valid_start));
assert property(@(posedge sclk) (cnt128d_rd[5:0] == 63) |-> (cnt128d_rd_valid_stop));
assert property(@(posedge sclk) (!rstn) |-> (cnt128d_rd_valid == 0));
assert property(@(posedge sclk) (cnt128d_rd_valid_start) |-> (cnt128d_rd_valid == 1));
assert property(@(posedge sclk) (cnt128d_rd_valid_stop) |-> (cnt128d_rd_valid == 0));
assert property(@(posedge sclk) (!rstn) |-> (cnt128d_rd == 0));
assert property(@(posedge sclk) (cnt128d_rd_valid) |-> (cnt128d_rd == $past(cnt128d_rd) + 1));
assert property(@(posedge sclk) (!rstn) |-> (mem_valid == 0));
assert property(@(posedge sclk) (cnt128d_rd_valid) |-> (mem_valid == 1));
assert property(@(posedge sclk) (!cnt128d_rd_valid) |-> (mem_valid == 0));
assert property(@(posedge sclk) (mem_valid) |-> (mem_data == rd_DATA[7:0]));
endmodule