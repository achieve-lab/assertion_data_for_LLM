<harm>
	<context name="default">

		<numeric clsEffort="0.1" exp="randomwalkfilter_::FilterCounter" loc="c,dt"/>
		<prop exp="randomwalkfilter_::Negative" loc="c,dt"/>
		<prop exp="randomwalkfilter_::MainClock" loc="c,dt"/>
		<prop exp="randomwalkfilter_::Positive" loc="c,dt"/>
		<prop exp="randomwalkfilter_::Lag" loc="c,dt"/>
		<prop exp="randomwalkfilter_::Lead" loc="c,dt"/>



		<template dtLimits="5D,3W,15A,-0.1E,R" exp="G({..#1&..}|-> P0)" /> 



		<sort name="causality" exp="1-afct/traceLength"/>
		<sort name="frequency" exp="atct/traceLength"/>
	</context>
</harm>



